		"custom": {
			"spacing": {
				"small": "max(1.25rem, 5vw)",
				"medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4rem, 10vw, 8rem)",
				"outer": "var(--wp--custom--spacing--small, 1.25rem)"
			},
			"typography": {
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"gigantic": "clamp(2.75rem, 6vw, 3.25rem)",
					"colossal": "clamp(3.25rem, 8vw, 6.25rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.6
				}
			}
		},
		"layout": {
			"contentSize": "1120px",
			"wideSize": "1855px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fontFamilies": [
				{
				  "fontFamily": "\".SFNS-Light\", \".SFNSText-Light\", \".HelveticaNeueDeskInterface-Light\", \".LucidaGrandeUI\", \"Segoe UI Light\", \"Ubuntu Light\", \"Roboto-Light\", \"DroidSans\", \"Tahoma\"",
				  "name": "GL System Font",
				  "slug": "gl-system-font"
				},
				{
				  "fontFamily": "\"Liberation Sans\", \"Nimbus Sans L\", \"FreeSans\", \"Helvetica Neue\", \"Helvetica\", \"Arial\"",
				  "name": "Narrow Sans Serif",
				  "slug": "narrow-sans-serif"
				},
				{
				  "fontFamily": "\"DejaVu Sans\", \"Bitstream Vera Sans\", \"Geneva\", \"Verdana\"",
				  "name": "Wide Sans Serif",
				  "slug": "wide-sans-serif"
				},
				{
				  "fontFamily": "\"Liberation Serif\", \"Nimbus Roman No 9 L\", \"FreeSerif\", \"Hoefler Text\", \"Times\", \"Times New Roman\", serif",
				  "name": "Narrow Serif",
				  "slug": "narrow-serif"
				},
				{
					"fontFamily": "\"Bitstream Charter\", \"URW Palladio L\", \"Cormorant Garamond\", \"FreeSerif\", \"Palatino\", \"Palatino Linotype\", \"Book Antiqua\", serif",
					"name": "Antique Serif",
					"slug": "antique-serif"
				},
				{
					"fontFamily": "\"DejaVu Serif\", \"Bitstream Vera Serif\", \"Century Schoolbook L\", \"Lucida Bright\", \"Georgia\", serif",
					"name": "Wide Serif",
					"slug": "wide-serif"
				},
				{
					"fontFamily": "\"Proza Libre\", \"Bitstream Charter\", \"URW Palladio L\", \"Cormorant Garamond\", \"FreeSerif\", \"Palatino\", \"Palatino Linotype\", \"Book Antiqua\", serif ",
					"name": "Heading Font",
					"slug": "heading-font"
				},
				{
					"fontFamily": " \"Fira Code\", \"Liberation Mono\", \"Nimbus Mono L\", \"FreeMono\", \"DejaVu Mono\", \"Bitstream Vera Mono\", \"Lucida Console\", \"Andale Mono\", \"Courier New\", monospace ",
					"name": "Better Monospace",
					"slug": "better-monospace"
				}
			],
			"fontSizes": [
				{
					"size": "1rem",
					"slug": "small"
				},
				{
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "x-large"
				}
			]
		}
