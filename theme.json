	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--gl-system-font)",
			"fontWeight": "400",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		},
		"blocks": {
			"core/paragraph": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wide-sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/group": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wide-sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}

			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--antique-serif)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"text": "var(--wp--preset--color--heading-foreground)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},

			"core/post-excerpt": {
				"elements": {
					"link": {
						"color": {
							"background": "var(--wp--preset--color--secondary)"
						},
						"spacing": {
							"padding": {
								"top": "0.6rem",
								"right": "1rem",
								"bottom": "0.6rem",
								"left": "1rem"
							}
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wide-sans-serif)",
					"fontWeight": "600",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)",
							"background": "var(--wp--preset--color--tertiary)"
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--gl-system-font)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "600",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "600",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)",
					"lineHeight": "var(--wp--custom--typography--line-height--small)"
				}
			},
			"h3": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "600",
					"fontSize": "var(--wp--custom--typography--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)"
				}
			},
			"h4": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--heading-font)",
					"fontWeight": "600",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"textTransform": "uppercase"
				}
			},
			"h5": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--gl-system-font)",
					"fontWeight": "600",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"color": {
						"text": "var(--wp--preset--color--heading-foreground)",
						"background": "var(--wp--preset--color--background)"
					},
					"fontFamily": "var(--wp--preset--font-family--gl-system-font)",
					"fontWeight": "400",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"textTransform": "uppercase"
				}
			},
			"p": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--wide-sans-serif)",
					"fontWeight": "400",
					"fontSize": "var(--wp--custom--typography--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--heading-foreground)",
					"background": "var(--wp--preset--color--tertiary)"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-large-dark",
			"title": "Header (Dark, large)",
			"area": "header"
		},
		{
			"name": "header-small-dark",
			"title": "Header (Dark, small)",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
